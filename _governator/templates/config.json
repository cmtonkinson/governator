{
  "project_mode": "",
  "remote_name": "origin",
  "default_branch": "main",
  "next_task_id": 1,
  "done_check": {
    "cooldown_seconds": 300,
    "last_check": 0
  },
  "last_update_at": "never",
  "planning": {
    "gov_hash": ""
  },
  "worker_timeout_seconds": 900,
  "worker_caps": {
    "global": 1
  },
  "reasoning_effort": {
    "default": "medium",
    "architect": "high",
    "planner": "high",
    "test_engineer": "low"
  },
  "agents": {
    "provider_by_role": {
      "default": "codex"
    },
    "providers": {
      "codex": {
        "bin": "codex",
        "args": [
          "--full-auto",
          "--search",
          "-c sandbox_workspace_write.network_access=true",
          "-c model_reasoning_effort={REASONING_EFFORT}",
          "exec",
          "--sandbox=workspace-write"
        ]
      },
      "claude": {
        "bin": "claude",
        "args": [
          "--print"
        ]
      },
      "gemini": {
        "bin": "gemini",
        "args": [
        ]
      }
    }
  }
}
